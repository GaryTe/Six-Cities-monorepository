{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@project/config": ["libs/config/src/index.ts"],
      "@project/const": ["libs/const/src/index.ts"],
      "@project/enum": ["libs/enum/src/index.ts"],
      "@project/error-validatio": ["libs/error-validatio/src/index.ts"],
      "@project/functions-validation": [
        "libs/functions-validation/functions-validation/src/index.ts"
      ],
      "@project/interface": ["libs/interface/src/index.ts"],
      "@project/jwt": ["libs/jwt/jwt/src/index.ts"],
      "@project/type": ["libs/type/src/index.ts"],
      "@project/upload-file": ["libs/upload-file/src/index.ts"],
      "@project/user-functions-interceptor": [
        "libs/functions-interceptor/user-functions-interceptor/src/index.ts"
      ],
      "@project/util": ["libs/util/src/index.ts"]
    },
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "tmp"]
}
